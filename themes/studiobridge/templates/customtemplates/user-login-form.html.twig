<div class="login-wrapper ">

    <div class="bg-pic">

        <img src="img/wallpapers/wallpaper.jpg" data-src="img/wallpapers/wallpaper.jpg" data-src-retina="img/wallpapers/wallpaper.jpg" alt="" class="lazy">


        <div class="bg-caption pull-bottom sm-pull-bottom text-white p-l-20 m-b-20">
            <h2 class="semi-bold text-white">
                Baltimore Airport</h2>
            <p class="small">
                Photograph by Austin Blair
            </p>
        </div>

    </div>


    <div class="login-container bg-white">
        <div class="p-l-50 m-l-20 p-r-50 m-r-20 p-t-50 m-t-30 sm-p-l-15 sm-p-r-15 sm-p-t-40">
            <img src="img/logo-white.png">
            <p class="p-t-35">Sign into your StudioBridge account</p>

            <form id="form-login" class="p-t-15" role="form" action="?ls" method="post" name="ls">

                <div class="form-group form-group-default">

                    <div class="controls">
                        {{ form.name }}
                    </div>
                </div>


                <div class="form-group form-group-default">

                    <div class="controls">
                        {{ form.pass }}
                    </div>
                </div>
                {{ form.form_build_id }}
                {{ form.form_id }}
                <div class="row">
                    <div class="col-md-6 no-padding">
                        <div class="checkbox ">
                            <input type="checkbox" value="1" id="checkbox1">
                            <label for="checkbox1">Keep Me Signed in</label>
                        </div>
                    </div>
                    <div class="col-md-6 text-right">
                        <a href="#" class="text-info small">Forgot Password?</a>
                    </div>
                </div>

                <button class="btn btn-primary btn-cons m-t-10" type="submit">Sign in</button>

            </form>

            <div class="pull-bottom sm-pull-bottom">
                <div class="col-sm-12 no-padding m-t-10">
                    <p>
                        <small>
                            If you do not have an account, contact Ashar at 4044

                        </small>
                        <br>
                        <small>
                            Production Version 1.0

                        </small>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>