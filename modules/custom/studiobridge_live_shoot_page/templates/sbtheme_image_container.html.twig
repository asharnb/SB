<article class="col-xs-12 col-md-12 col-lg-12 ">

    <div id="portlet-linear-color" class="panel panel-default">
        <div class="panel-heading seperator">
            <div class="panel-title">Product Images
            </div>
            <div class="panel-controls">

                {% spaceless %}
                    {%
                    set classes = [
                    'btn', 'btn-sm',
                    type == 'submit' ? 'js-form-submit',
                    icon and icon_position and not icon_only ? 'icon-' ~ icon_position,
                    ]
                    %}
                    {% block input %}
                        {% if icon_only %}
                            <button{{ attributes.addClass(classes, 'icon-only') }}>
                                <span class="sr-only">{{ label }}</span>
                                {{ icon }}
                            </button>
                        {% else %}
                            {% if icon_position == 'after' %}
                                <button{{ attributes.addClass(classes) }}>{{ label }}{{ icon }}</button>{{ children }}
                            {% else %}
                                <button{{ attributes.addClass(classes) }}>{{ icon }}{{ label }}</button>{{ children }}
                            {% endif %}
                        {% endif %}
                        {{ children }}
                    {% endblock %}
                {% endspaceless %}





            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div id="imagecontainer" name="imagecontainer" class="ui-sortable">

                </div>

            </div>
        </div>


</article>