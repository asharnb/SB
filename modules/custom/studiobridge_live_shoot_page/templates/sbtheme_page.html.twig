{{ form.form_build_id }}
                {{ form.form_id }}
<div class="row" style="min-height: 2024px;">
    <div class="col-xs-10 shadow-right"
         style="min-height: 2024px;box-shadow: 0 4px 5px -2px rgba(0,0,0,.2),0 7px 10px 1px rgba(0,0,0,.14),0 2px 16px 1px rgba(0,0,0,.12); padding-top:10px;">
        <div class="row">

            <article class="col-xs-3 col-md-3 col-lg-3 ">

                {% block sbtheme_scancontainer %}

                {% endblock %}

            </article>
            <!-- information -->
            <article class="col-xs-9 col-md-9 col-lg-9 ">


                {% block sbtheme_product %}
                {% endblock %}

            </article>

        </div>

        <div class="row">


            <article class="col-xs-12 col-md-12 col-lg-12 ">

                    <div id="portlet-linear-color" class="panel panel-default">
                        <div class="panel-heading seperator">
                            <div class="panel-title">Product Images
                            </div>
                            <div class="panel-controls">


                                {{ form.resequence }}
                                {{ form.delete }}


                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row" id="imagecontainer-wrapper">
                                <div id="imagecontainer" name="imagecontainer" class="ui-sortable">

                                    {% block sbimages %}
                                    {% endblock sbimages %}

                                </div>
                            </div>

                        </div>
                    </div>


            </article>
        </div>
    </div>
    <!-- right panel -->
    <div class="col-xs-2">
        {% block sidepanel %}
        {% endblock sidepanel %}

    </div>
</div>